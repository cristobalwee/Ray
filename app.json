{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-font",
      {
        "fonts": [
          "assets/fonts/MartinaPlantijn-Black.ttf",
          "assets/fonts/Metric-Light.ttf",
          "assets/fonts/Metric-Regular.ttf",
          "assets/fonts/Metric-Medium.ttf"
        ]
      }],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
